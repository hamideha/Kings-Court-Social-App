{
  "name": "chat-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "server": "cd ../Server && nodemon app.js",
    "start": "craco start --config ./craco.config.js",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "dev": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run start\""
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@headlessui/react": "^1.1.1",
    "@heroicons/react": "^1.0.1",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "graphql": "^15.5.0",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-query": "^3.15.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "subscriptions-transport-ws": "^0.9.18",
    "zustand": "^3.5.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^9",
    "concurrently": "^6.1.0",
    "eslint-config-react-app": "^6.0.0",
    "postcss": "^7",
    "tailwind-scrollbar-hide": "^1.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "installConfig": {
    "pnp": true
  },
  "proxy": "http://localhost:4000/"
}
